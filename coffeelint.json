{
  "arrow_spacing": {
    "level": "error"
  },
  "braces_spacing": {
    "spaces": 1,
    "level": "warn"
  },
  "no_empty_param_list": {
    "level": "error"
  },
  "no_tabs": {
    "level": "error"
  },
  "no_trailing_whitespace": {
    "level": "error"
  },
